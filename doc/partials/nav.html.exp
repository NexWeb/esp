        <div class="column">
            <div class="inverted secondary pointing ui menu">
                <div class="header left item">
                    <a href="/index.html">Embedthis ESP</a>
                </div>
                <div class="right menu">
                    <div class="ui top right">
                        <a class="item" href="http://goo.gl/ZQsfIC">News</a>
                        <a class="item" href="https://embedthis.com/esp/">ESP Site</a>
                        <a class="item" href="https://github.com/embedthis/esp">Repository</a>
                        <a class="item" href="https://embedthis.com/blog/">Blog</a>
                        <a class="item" href="https://twitter.com/mobstream">Twitter</a>
                    </div>
                </div>
            </div>
            <div class="ui breadcrumb">
                <a class="section" href="@@top">Home</a>
                <% for each (crumb in meta.crumbs ) { 
                    for (let [name, url] in crumb ) { %>
                <div class="divider">/</div>
                <a class="section" href="<%= url %>">
                    <%= name %>
                </a>
                <% } } %>
                <div class="divider">/</div>
                <a class="section active" href="<%= meta.basename %>"><%= meta.cookie || meta.title %></a>
            </div>
        </div>
        <div class="ui black small launch right attached toggle button" style="width: 40px;">
            <i class="icon list layout">
                <span class="text" style="display: none;">Menu</span>
            </i>
        </div>

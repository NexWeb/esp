{
    "name": "esp",
    "description": "Embedthis ESP, the amazingly fast C-Language web framework",
    "title": "Embedthis ESP",
    "version": "1.0.0-rc0",
    "keywords": [
        "esp",
        "package"
    ],
    "homepage": "http://embedthis.com/esp",
    "author": {
        "name": "Embedthis Software",
        "email": "dev@embedthis.com",
        "url": "http://embedthis.com"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/esp.git"
    },
    "bugs": {
        "url": "http://github.com/embedthis/esp/issues"
    },
    "dependencies": {
        "appweb": "~5.0",
        "http": "*",
        "me-dev": "~0.8",
        "mpr": "*",
        "osdep": "~0.8",
        "pcre": "*"
    },
    "optionalDependencies": {
        "esp-html-mvc": "~1.0",
        "esp-mvc": "~1.0",
        "esp-server": "~1.0",
        "est": "~0.6",
        "me-ssl": "~0.8",
        "sqlite": "~1"
    },
    "devDependencies": {
        "appweb": "*",
        "me": ">0.8.0"
    },
    "dirs": {
        "paks": "src/paks",
        "client": "client"
    },
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "http://embedthis.com/licensing/"
        }
    ],
    "paks": {
        "appweb": {
            "noblend": true,
            "noexport": true,
            "nodeps": true
        },
        "esp-server": {
            "noblend": true,
            "noexport": true
        },
        "esp-mvc": {
            "noblend": true,
            "noexport": true
        },
        "esp-html-mvc": {
            "noblend": true,
            "noexport": true
        }
    },
    "esp": {
        "combined": false,
        "update": true,
        "serverPrefix": "/do",
        "server": {
            "generate": {
                "appweb": "esp-mvc/appweb.conf",
                "migration": "esp-mvc/migration.c",
                "controller": "esp-mvc/controller.c",
                "controller-singleton": "esp-mvc/controller.c",
                "files": [
                    "esp-mvc/src/app.c"
                ]
            }
        }
    }
}

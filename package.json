{
    "name": "esp",
    "description": "Embedthis ESP, the amazingly fast C-Language web framework",
    "title": "Embedthis ESP",
    "version": "5.4.0",
    "keywords": [
        "esp",
        "package"
    ],
    "homepage": "https://embedthis.com/esp",
    "author": {
        "name": "Embedthis Software",
        "email": "dev@embedthis.com",
        "url": "https://embedthis.com"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/esp.git"
    },
    "bugs": {
        "url": "http://github.com/embedthis/esp/issues"
    },
    "pak": "~0.10",
    "dependencies": {
        "http": "~6.0",
        "mpr": "~6.0",
        "osdep": "~0.8",
        "pcre": "~1.0"
    },
    "optionalDependencies": {
        "appweb": "~5",
        "est": "~0.6",
        "ssl": "~0.8",
        "sqlite": "~1"
    },
    "devDependencies": {
        "me": ">0.8.0"
    },
    "directories": {
        "paks": "src/paks",
        "client": "client"
    },
    "export": [
        {
            "from": "doc/**",
            "to": "${TOP}"
        }
    ],
    "licenses": [
        {
            "type": "GPL",
            "url": "http://www.gnu.org/licenses/gpl-2.0.html"
        },
        {
            "type": "Commercial",
            "url": "https://embedthis.com/licensing/"
        }
    ],
    "app": {
        "appweb": {
            "noblend": true,
            "noexport": true,
            "nodeps": true
        },
        "*": {
            "noblend": true,
            "ignore": [
                "doc**"
            ]
        }
    }
}
